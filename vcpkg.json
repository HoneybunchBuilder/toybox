{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "toybox",
  "version": "0.1.0",
  "dependencies": [
    "cgltf",
    "enkits",
    "flecs",
    "imgui",
    "joltphysics",
    "json-c",
    "ktx",
    {
      "name": "ktx",
      "host": true,
      "features": [
        "vulkan",
        "tools"
      ]
    },
    "meshoptimizer",
    {
      "name": "meshoptimizer",
      "host": true,
      "features": [
        "gltfpack"
      ]
    },
    "mimalloc",
    {
      "name": "sdl3",
      "features": [
        "vulkan"
      ]
    },
    {
      "name": "sdl3-mixer",
      "default-features": false
    },
    "steam-audio",
    "tracy",
    "volk",
    "vulkan-memory-allocator"
  ],
  "overrides": [
    {
      "name": "cgltf",
      "version": "1.13"
    },
    {
      "name": "enkits",
      "version": "1.11",
      "port-version": 3
    },
    {
      "name": "imgui",
      "version": "1.89.9"
    },
    {
      "name": "joltphysics",
      "version": "4.0.0"
    },
    {
      "name": "json-c",
      "version": "2023-08-12"
    },
    {
      "name": "ktx",
      "version": "4.3.2"
    },
    {
      "name": "meshoptimizer",
      "version": "0.20"
    },
    {
      "name": "mimalloc",
      "version": "2.1.2"
    },
    {
      "name": "sdl3",
      "version": "3.0.0-534f753"
    },
    {
      "name": "sdl3-mixer",
      "version": "3.0.0-087004f"
    },
    {
      "name": "tracy",
      "version": "0.10.0"
    },
    {
      "name": "volk",
      "version": "1.3.275.0"
    },
    {
      "name": "vulkan-memory-allocator",
      "version": "3.0.1"
    }
  ]
}