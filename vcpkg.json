{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "toybox",
  "version": "0.1.0",
  "dependencies": [
    "joltphysics",
    "flecs",
    "cgltf",
    "imgui",
    "ktx",
    {
      "name": "meshoptimizer"
    },
    {
      "name": "meshoptimizer",
      "host": true,
      "features": [
        "gltfpack"
      ]
    },
    "mimalloc",
    {
      "name": "sdl2",
      "features": [
        "vulkan"
      ]
    },
    {
      "name": "sdl2-mixer",
      "default-features": false
    },
    "tracy",
    "volk",
    "vulkan-memory-allocator",
    {
      "name": "ktx",
      "host": true,
      "features": [
        "vulkan",
        "tools"
      ]
    },
    "json-c"
  ],
  "overrides": [
    {
      "name": "cgltf",
      "version": "1.13"
    },
    {
      "name": "imgui",
      "version": "1.89.9"
    },
    {
      "name": "ktx",
      "version": "4.1.0-rc2"
    },
    {
      "name": "meshoptimizer",
      "version": "0.19",
      "port-version": 1
    },
    {
      "name": "mimalloc",
      "version": "2.0.7"
    },
    {
      "name": "sdl2",
      "version": "2.28.4"
    },
    {
      "name": "tracy",
      "version": "0.10.0"
    },
    {
      "name": "volk",
      "version": "1.3.250"
    },
    {
      "name": "vulkan-memory-allocator",
      "version": "3.0.1"
    },
    {
      "name": "json-c",
      "version": "2022-06-26",
      "port-version": 3
    },
    {
      "name": "joltphysics",
      "version": "3.0.1",
      "port-version": 1
    }
  ]
}