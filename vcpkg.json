{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "toybox",
  "version": "0.1.0",
  "dependencies": [
    "cgltf",
    "imgui",
    {
      "name": "ktx",
      "features": [
        "vulkan"
      ]
    },
    {
      "name": "meshoptimizer",
      "host": true,
      "features": [
        "gltfpack"
      ]
    },
    "mimalloc",
    {
      "name": "sdl2",
      "features": [
        "vulkan"
      ]
    },
    "sdl2-image",
    "tracy",
    "volk",
    "vulkan-memory-allocator",
    {
      "name": "ktx",
      "host": true,
      "features": [
        "vulkan",
        "tools"
      ]
    },
    "json-c"
  ],
  "overrides": [
    {
      "name": "cgltf",
      "version": "1.13"
    },
    {
      "name": "imgui",
      "version": "1.87"
    },
    {
      "name": "ktx",
      "version": "4.1.0-rc2"
    },
    {
      "name": "meshoptimizer",
      "version": "0.18"
    },
    {
      "name": "mimalloc",
      "version": "2.0.6"
    },
    {
      "name": "sdl2",
      "version": "2.0.22"
    },
    {
      "name": "sdl2-image",
      "version": "2.0.5",
      "port-version": 6
    },
    {
      "name": "tracy",
      "version": "0.8.2"
    },
    {
      "name": "volk",
      "version": "1.3.204"
    }
  ]
}